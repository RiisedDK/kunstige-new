ys(startDate, daysToAdd) {\n  var date = new Date(startDate.getTime());\n  var count = 0;\n  while (count < daysToAdd) {\n    date.setDate(date.getDate() + 1);\n    if (date.getDay() >= 1 && date.getDay() <= 5) { \/\/ Mandag til Fredag\n      count++;\n    }\n  }\n  return date;\n}\n\nfunction formatDate(date) {\n  var dayNames = ['Søndag', 'Mandag', 'Tirsdag', 'Onsdag', 'Torsdag', 'Fredag', 'Lørdag'];\n  var dayOfWeek = dayNames[date.getDay()];\n  var day = date.getDate();\n  var month = date.getMonth() + 1; \/\/ Måneder starter fra 0\n  return dayOfWeek + ' d. ' + (day < 10 ? '0' : '') + day + '\/' + (month < 10 ? '0' : '') + month;\n}\n\nwindow.onload = function() {\n  var stockStatusElement = document.getElementById('stockInfo');\n  var deliveryDateElement = document.getElementById('deliveryInfo');\n\n  var circleSpan = document.createElement('span');\n  circleSpan.classList.add('circle', isProductInStock ? 'green' : 'red');\n  stockStatusElement.appendChild(circleSpan);\n  \n  var stockText = document.createTextNode(isProductInStock ? 'På lager' : 'Udsolgt');\n  stockStatusElement.appendChild(stockText);\n  \n  \/\/ Viser kun forventet leveringsdato, hvis produktet er på lager\n  if (isProductInStock) {\n    var deliveryDate = addBusinessDays(new Date(), 2); \/\/ Ændret til 2 dage\n    var deliveryText = document.createTextNode('Forventet leveret inden ');\n    var dateInfoSpan = document.createElement('span');\n    dateInfoSpan.classList.add('date-info');\n    dateInfoSpan.textContent = formatDate(deliveryDate);\n\n    deliveryDateElement.appendChild(deliveryText);\n    deliveryDateElement.appendChild(dateInfoSpan);\n  }\n};\n<\/script>\n\n<\/body>\n<\/html>"}},"faa2b3e6-c0a1-4269-baa6-d2605efe4c05":{"type":"quantity_selector","settings":{"qty_style":"1","qty_layout":"2","spacing_top":0,"spacing_bottom":10}},"f1b31d90-1e43-4948-9fe3-97ba43f6fb34":{"type":"buy_buttons","settings":{"show_dynamic_checkout":false,"spacing_top":0,"spacing_bottom":15,"atc_animation"